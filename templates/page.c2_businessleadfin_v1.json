    "1654102552227275fa": {
      "type": "faq",
      "blocks": {
        "16541025525b9666bc-0": {
          "type": "question",
          "settings": {
            "title": "What can students do with this certification?",
            "text": "<p>This certification can be used on resumes and portfolios to demonstrate that a student has trained in, and demonstrated high levels of achievement in, business, finance, and leadership. <\/p>"
          }
        },
        "e25bef5d-4267-4426-8627-7afd528dff33": {
          "type": "question",
          "settings": {
            "title": "Do all of my application courses have to belong to the same category?",
            "text": "<p>No, students may take any business or finance courses present on this page to count for their certificate. <\/p>"
          }
        },
        "16541025525b9666bc-1": {
          "type": "question",
          "settings": {
            "title": "If I have already taken a course, can it count towards the certificate?",
            "text": "<p>Please email your completed worksheet with a list of classes taken to info@ivycamps.org.<\/p>"
          }
        },
        "e4c9cc6c-c829-419a-a7c4-07bd59c4898e": {
          "type": "question",
          "settings": {
            "title": "Will I receive a copy of my certificate?",
            "text": "<p>For students in the USA, we will be mailing a physical certificate upon completion. For students in other parts of the world, we will email a certificate that can be printed and displayed.<\/p>"
          }
        }
      },
