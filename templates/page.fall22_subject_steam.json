    "1659706135947fb274": {
      "type": "text-and-image",
      "settings": {
        "image": "shopify:\/\/shop_images\/50.png",
        "image_width": 550,
        "subtitle": "",
        "title": "Online Class Format",
        "text": "<p>● All classes are held <strong>live and online<\/strong><br\/><br\/>● Classes are <strong>90 minutes<\/strong> in duration<br\/><br\/>● Classes happen <strong>once per week<\/strong>, on the <strong>same day<\/strong> each week<br\/><br\/>● All classes will have a small class size between <strong>1-6 students<\/strong><\/p>",
        "button_label": "",
