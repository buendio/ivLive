    "1652906461f7aa78b9": {
      "type": "faq",
      "blocks": {
        "1652906461c9ebb360-0": {
          "type": "question",
          "settings": {
            "title": "What can students do with these certifications?",
            "text": "<p>These certification can be used on resumes and portfolios to demonstrate that a student has trained in, and demonstrated high levels of achievement in the subject matter of their certificate.<\/p>"
          }
        },
        "1652906461c9ebb360-1": {
          "type": "question",
          "settings": {
            "title": "If I have already taken a course, can it count towards the certificate?",
            "text": "<p>Absolutely! Please email info@ivycamps.org to learn more.<\/p>"
          }
        },
        "6ac29def-8bf2-47fd-a18b-a9089511e6ac": {
          "type": "question",
          "settings": {
            "title": "Will I receive a copy of my certificate?",
            "text": "<p>For students in the USA, we will be mailing a physical certificate upon completion. For students in other parts of the world, we will email a certificate that can be printed and displayed. <\/p>"
          }
        }
      },
